{
  "name": "airbnb-backend",
  "version": "1.0.0",
  "description": "Backend for Airbnb-like camping platform",
  "main": "src/server.js",  "scripts": {
    "start": "node scripts/check-readiness.js && node src/server.js",
    "start:production": "node scripts/start-production.js",
    "dev": "nodemon src/server.js",
    "test": "jest",
    "prisma:generate": "prisma generate --schema=./prisma/schema.prisma",
    "prisma:migrate": "prisma migrate dev --schema=./prisma/schema.prisma",
    "prisma:studio": "prisma studio --schema=./prisma/schema.prisma",
    "prisma:deploy": "prisma migrate deploy --schema=./prisma/schema.prisma",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "build": "npm run prisma:generate",
    "smoke-test": "node scripts/smoke-test.js",
    "railway": "node scripts/railway-healthcheck.js",
    "postinstall": "prisma generate --schema=./prisma/schema.prisma",
    "validate:env": "node scripts/validate-environment.js",
    "check:system": "node scripts/system-check.js",
    "check:health": "node scripts/check-health.js",
    "prestart": "node -e \"process.env.SKIP_ENV_VALIDATION || require('./scripts/validate-environment.js')\"",
    "predeploy": "npm run validate:env && npm run check:system && npm run check:health"
  },
  "dependencies": {
    "@ioredis/commands": "^1.2.0",
    "@prisma/client": "^5.0.0",
    "@supabase/supabase-js": "^2.49.4",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^3.0.2",
    "cloudinary": "^2.6.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "denque": "^2.1.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "form-data": "^4.0.2",
    "google-auth-library": "^9.15.1",
    "googleapis": "^148.0.0",
    "helmet": "^7.0.0",
    "ioredis": "^5.6.1",
    "jsonwebtoken": "^9.0.2",
    "jwks-rsa": "^3.2.0",
    "lodash.defaults": "^4.2.0",
    "lodash.isarguments": "^3.1.0",
    "mailgun.js": "^9.4.1",
    "multer": "^1.4.5-lts.2",
    "natural": "^8.0.1",
    "node-cron": "^3.0.3",
    "node-nlp": "^5.0.0-alpha.5",
    "nodemailer": "^7.0.3",
    "rate-limit-redis": "^4.2.0",
    "redis-errors": "^1.2.0",
    "redis-parser": "^3.0.0",
    "standard-as-callback": "^2.1.0",
    "stripe": "^18.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "axios": "^1.9.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "fs-extra": "^11.3.0",
    "jest": "^29.5.0",
    "nodemon": "^2.0.22",
    "prettier": "^3.2.5",
    "prisma": "^5.0.0"
  }
}
